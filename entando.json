{
    "microservices": [
        {
            "name": "strapi-connector-ms",
            "stack": "node",
            "healthCheckPath": "/api/health",
            "dbms": "postgresql",
            "commands": {
                "run": "npm run dev"
            },
            "env": [{
                "name": "APP_ORIGIN",
                "value": "http://giovanni.k8s-entando.org"
            },
            {
                "name": "LOG_LEVEL",
                "value": "debug"
            }]
        }
    ],
    "microfrontends": [
        {
            "name": "strapi-content-list",
            "customElement": "strapi-content-list",
            "stack": "react",
            "type": "widget",
            "group": "free",
            "publicFolder": "public",
            "titles": {
                "en": "strapi-content-list",
                "it": "strapi-content-list"
            }
        }
    ],
    "svc": [
        "postgresql",
        "keycloak"
    ],
    "name": "entando-strapi-connector",
    "version": "0.0.3",
    "description": "entando-strapi-connector description",
    "type": "bundle"
}