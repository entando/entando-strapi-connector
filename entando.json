{
    "microservices": [
        {
            "name": "strapi-connector-ms",
            "stack": "node",
            "healthCheckPath": "/api/health",
            "dbms": "postgresql",
            "commands": {
                "run": "npm run dev"
            },
            "env": [
                {
                    "name": "APP_ORIGIN",
                    "value": "http://my-portal.entando.org"
                },
                {
                    "name": "LOG_LEVEL",
                    "value": "info"
                },
                {
                    "name": "API_CONFIG_TABLE",
                    "value": "api_config"
                }
            ]
        }
    ],
    "microfrontends": [
        {
            "name": "strapi-content-list",
            "customElement": "strapi-content-list",
            "stack": "react",
            "type": "widget",
            "group": "free",
            "publicFolder": "public",
            "commands": {
                "run": "npm run dev"
            },
            "titles": {
                "en": "strapi-content-list",
                "it": "strapi-content-list"
            },
            "configMfe": "strapi-content-list-config"
        },
        {
            "name": "strapi-content-list-config",
            "customElement": "strapi-content-list-config",
            "stack": "react",
            "type": "widget-config",
            "group": "free",
            "publicFolder": "public",
            "commands": {
                "run": "npm run dev"
            }
        },
        {
            "name": "strapi-content",
            "customElement": "strapi-content",
            "stack": "react",
            "type": "widget",
            "group": "free",
            "publicFolder": "public",
            "commands": {
                "run": "npm run dev"
            },
            "titles": {
                "en": "strapi-content",
                "it": "strapi-content"
            },
            "configMfe": "strapi-content-config"
        },
        {
            "name": "strapi-content-config",
            "customElement": "strapi-content-config",
            "stack": "react",
            "type": "widget-config",
            "group": "free",
            "publicFolder": "public",
            "commands": {
                "run": "npm run dev"
            }
        },
        {
            "name": "strapi-config",
            "customElement": "strapi-config",
            "stack": "react",
            "type": "app-builder",
            "slot": "content",
            "group": "free",
            "publicFolder": "public",
            "paths": [
                "/settings",
                "/content-template"
            ],
            "commands": {
                "run": "npm run dev"
            },
            "nav": [
                {
                    "label": {
                        "it": "Impostazioni",
                        "en": "Settings"
                    },
                    "target": "internal",
                    "url": "/settings"
                },
                {
                    "label": {
                        "it": "Template contenuti",
                        "en": "Content Template"
                    },
                    "target": "internal",
                    "url": "/content-template"
                }
            ]
        }
    ],
    "svc": [
        "postgresql",
        "keycloak"
    ],
    "name": "entando-strapi-connector",
    "version": "0.0.3",
    "description": "entando-strapi-connector description",
    "type": "bundle"
}